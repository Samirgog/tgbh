{
  "name": "tgbh",
  "private": false,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "configbot": "node configure.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint-staged": "lint-staged",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:es:fix": "npm run lint:es -- --fix",
    "prettier:check": "prettier ./src/. --check",
    "prettier:fix": "prettier src/ --write",
    "check-types": "tsc",
    "pre-commit": "npm run lint-staged",
    "format": "npm run lint:es:fix && npm run prettier:fix"
  },
  "dependencies": {
    "@types/telegram-web-app": "^7.3.1",
    "buffer": "^6.0.3",
    "dotenv": "^16.4.5",
    "framer-motion": "^12.5.0",
    "gh-pages": "^5.0.0",
    "lucide-react": "^0.482.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-preview": "^9.0.0",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^6.30.0",
    "styled-components": "^5.3.11",
    "uuid": "^11.1.0",
    "vite-plugin-node-polyfills": "^0.7.0",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-styled-components-a11y": "^2.2.0",
    "eslint-plugin-xss": "^0.1.12",
    "prettier": "3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  }
}
